class Foo {}
var foo = Foo();
print foo; // <Foo instance>

//CASE property_set
class Foo {}
var foo = Foo();
foo.a = "lol";
print foo.a; // lol

//CASE no_this_method
class Foo {
    lol(val) {
        print val;
    }
}

var foo = Foo();
foo.lol("lmao"); // > lmao

//CASE this_method
class Foo {
    set_lol(val) {
        this.lol = val;
    }

    get_lol() {
        return this.lol;
    }
}

var foo = Foo();
foo.set_lol("lol");
print foo.get_lol(); // > lol
