class Foo {}
var foo = Foo();
print foo; // <Foo instance>

//CASE property_set
class Foo {}
var foo = Foo();
foo.a = "lol";
print foo.a; // lol

//CASE no_this_method
class Foo {
    lol(val) {
        print val;
    }
}

var foo = Foo();
foo.lol("lmao"); // > lmao

//CASE this_method
class Foo {
    set_lol(val) {
        this.lol = val;
    }

    get_lol() {
        return this.lol;
    }
}

var foo = Foo();
foo.set_lol("lol");
print foo.get_lol(); // > lol

//CASE initializer
class Foo {
    init(val) {
        this.val = val;
    }
}

var foo = Foo("lol");
print foo.val; // > lol

//CASE explicit_reinitialize
class Foo {
    init() {
        print this;
    }
}

// Should print three times
var foo = Foo();
print foo.init();

//CASE init_with_early_return
class Foo {
    init() {
        this.val = 0;
        return;
    }
}

var foo = Foo();
print foo.val; // > 0
